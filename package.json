{
  "name": "express-starter",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "node --no-warnings --es-module-specifier-resolution=node --watch src",
    "start": "node --es-module-specifier-resolution=node src",
    "dev:migrate": "prisma migrate dev --name init",
    "deploy:migrate": "prisma migrate deploy",
    "generate": "prisma generate",
    "lint": "eslint .",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@prisma/client": "^5.13.0",
    "dotenv": "^16.3.2",
    "ejs": "3.1.7",
    "express": "^4.18.3",
    "express-validate-kit": "^0.0.6",
    "joi": "^17.8.4",
    "md5": "^2.3.0",
    "moment": "^2.29.4",
    "morgan": "~1.9.1",
    "serve-favicon": "^2.5.0",
    "setup-router": "^0.1.10"
  },
  "devDependencies": {
    "@sunshj/eslint-config": "^1.0.6",
    "@sunshj/prettier-config": "^1.0.0",
    "@types/express": "^4.17.21",
    "@types/node": "^20.12.8",
    "eslint": "^8.57.0",
    "lint-staged": "^15.2.2",
    "prettier": "^2.8.8",
    "prisma": "^5.13.0",
    "simple-git-hooks": "^2.11.1"
  },
  "engines": {
    "node": "^18.15.0"
  },
  "imports": {
    "#*": "./src/*"
  },
  "lint-staged": {
    "src/**/*.js": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "prettier": "@sunshj/prettier-config",
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  }
}
